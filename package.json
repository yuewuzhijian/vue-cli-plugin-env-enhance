{
  "name": "vue-cli-plugin-env-enhance",
  "version": "1.0.0",
  "description": "A vue-cli plugin enhances env for a vite-like experience, also supporting the use of 'import.meta.env'",
  "author": "yuewuzhijian",
  "license": "MIT",
  "homepage": "https://github.com/yuewuzhijian/vue-cli-plugin-env-enhance",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuewuzhijian/vue-cli-plugin-env-enhance.git"
  },
  "keywords": [
    "vue",
    "vue-cli",
    "plugin",
    "env",
    "enhance"
  ],
  "exports": {
    ".": "./dist/index.cjs",
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.cjs"
    }
  },
  "main": "dist/index.cjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint . --ext .js,.cjs,.mjs,.ts --fix",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "publish": "npm publish --registry https://registry.npmjs.org"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vue/cli-service": "^5.0.8",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^12.0.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "minimist": "^1.2.8",
    "prettier": "^3.4.1",
    "rollup": "^4.28.0",
    "rollup-plugin-dts": "^6.1.1",
    "typescript": "^5.7.2"
  },
  "overrides": {
    "prettier": "^3.4.1"
  }
}
